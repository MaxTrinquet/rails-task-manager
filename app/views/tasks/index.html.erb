<h1>My Tasks </h1>
<div class="tasks">
  <% @tasks.each do |task| %>
    <div class="task">
    <%if task.completed == true%>
      <i class="far fa-square"></i><p><%=task.title%></p>
      <% else %>
        <i class="far fa-check-square"></i> <p><%=task.title%></p>
      <% end %>
      </div>
    <% end %>
</div>
